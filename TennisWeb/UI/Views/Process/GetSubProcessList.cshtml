@model List<ProcessListDto>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@{
    if (TempData["ResponseMessage"] != null)
    {
        <div class="bg-primary text-white text-center lead">
            @TempData["ResponseMessage"]
        </div>
    }
}

@section Links{
<link rel="stylesheet" type="text/css" href="~/assets/css/ProcessCard.css" />
<style>
    .radial-gradient {
        background-image: radial-gradient(farthest-corner at 40px 40px, #f35 0%, #43e 100%);
    }

    .card img {
        width: 100%;
    }

    .card{
        background-color: #2b325800;
    }
    
</style>
}

<div class="wrapper">
    <div class="cards">
        <h1 class="pen-title">
            Image <br />
            Shadow
        </h1>
        <div class="card">
            <div class="card-content">
                <div class="top">
                    <h3 class="name">Jeffery Rogers</h3>
                    <div class="date"><span>06 September</span></div>
                </div>
                <div class="bottom">
                    <h2>Toughening the Mind by Challenging the Body</h2>
                    <div class="bookmark">
                        <svg xmlns="http://www.w3.org/2000/svg" x="0px" y="0px" width="24" height="24"
                            viewBox="0 0 24 24" class="bookmark-icn">
